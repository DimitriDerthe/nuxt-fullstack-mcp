{
  "name": "nuxt-fullstack-mcp",
  "type": "module",
  "version": "0.2.4",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r run dev",
    "play": "pnpm -C packages/nuxt-mcp play",
    "lint": "eslint",
    "release": "bumpp -r && pnpm -r publish",
    "test": "vitest",
    "test:integration": "node packages/nuxt-mcp/test-integration.ts",
    "inspect": "npx @modelcontextprotocol/inspector",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks && nr -r dev:prepare",
    "docker:build": "docker build -t nuxt-fullstack-mcp .",
    "docker:run": "docker run -p 3000:3000 nuxt-fullstack-mcp",
    "docker:dev": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker system prune -a",
    "start": "NODE_ENV=production node packages/nuxt-mcp/playground/.output/server/index.mjs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.1.0",
    "@antfu/ni": "^25.0.0",
    "@antfu/utils": "^9.2.0",
    "@modelcontextprotocol/inspector": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@types/node": "latest",
    "bumpp": "^10.2.2",
    "eslint": "^9.32.0",
    "h3": "^1.15.4",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "simple-git-hooks": "^2.13.1",
    "tinyexec": "^1.0.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "unbuild": "^3.6.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "vitest-package-exports": "^0.1.1",
    "yaml": "^2.8.0",
    "zod": "^3.25.76"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
